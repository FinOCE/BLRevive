{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2017",
      "strict": true,
      "rootDir": ".",
      "typeRoots": [
        "./types"
      ],
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "exclude": [
      "./node_modules"
    ]
  }
  